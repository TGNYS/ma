	Name = "QQ群56117318118点击复制",
	Callback = function()
print("执行(复制561173181)并弹出(复制成功-你已复制QQ群号)")
setclipboard("561173181")
OrionLib:MakeNotification({
                    Name = "复制成功", 
                    Content = "你已复制QQ群号",
                    Time = 10 -- 持续时间
                })    
                local sound = Instance.new("Sound", workspace)
sound.SoundId = "rbxassetid://4590662766"
sound:Play()
end
})    
