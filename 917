local announcement = Window:MakeTab({
    Name = "主页",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
announcement:AddParagraph("作者","萧炎,凡尘")
announcement:AddParagraph("版本","高级")
announcement:AddParagraph("警告","别去死铁轨执行这个脚本")
